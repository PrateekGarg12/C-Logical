#include <stdio.h>

int main() {
    //arithmetic
   int a,b,c;
   printf("enter the number of a & b :");
   scanf("%d %d",&a,&b);
   
   c=a+b;
   printf("a & b addition is : %d",c);
   printf("\n");
   
   int num1,num2,num3;
   printf("enter the number of a & b :");
   scanf("%d %d",&num1,&num2);
   
   num3=num1-num2;
   printf("num1 & num2 subtaction is : %d",num3);
   printf("\n");
   
   int j,k,l;
   printf("enter the number of j & k :");
   scanf("%d %d",&j,&k);
   
   l=j*k;
   printf("j & l multiplication is : %d",l);
   printf("\n");
   
   int s,d,f;
   printf("enter the number of s & d :");
   scanf("%d %d",&s,&d);
   
   f=s/d;
   printf("s & d divide is : %d",f);
   return 0;
   
}
